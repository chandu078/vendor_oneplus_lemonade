            "sensor_arch":"qcom_sensor_see"
        },
        "sensor_config":{
            "android.sensor.accelerometer":{
                "type":1,
                "support":"true",
                "module":"bmi160|icm4x6xx",
                "bmi160":{
                    "mode":"batch_cali|factory_cali",
                    "offset_x": 0.0,
                    "offset_y": 0.0,
                    "offset_z": 0.0
                },
                "icm4x6xx":{
                    "mode":"batch_cali|factory_cali",
                    "offset_x": 0.0,
                    "offset_y": 0.0,
                    "offset_z": 0.0
                }
            },
            "android.sensor.magnetic_field":{
                "type":2,
                "support":"true",
                "module":"ak0991x|mmc56x3x",
                "ak0991x":{
                    "mode":"batch_cali"
                },
                "mmc56x3x":{
                    "mode":"batch_cali"
                }
            },
            "android.sensor.gyroscope":{
                "type":4,
                "support":"true",
                "module":"bmi160|icm4x6xx",
                "bmi160":{
                    "mode":"batch_cali|factory_cali",
