            },
            "qti.sensor.rgb":{
                "type":33171001,
                "support":"true",
                "module":"tcs3701_rgb",
                "tcs3701_rgb":{
                    "mode":"factory_cali|scale_cali",
                    "scale":0,
                    "cali_para_ver":0
